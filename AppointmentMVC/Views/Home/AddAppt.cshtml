@model AppointmentMVC.Model.Appointment
@{
    ViewBag.Title = "AddAppt";
}

<h2>Add an Appointment</h2>
<form method="post">
    @Html.LabelFor(x => x.Description) <br />
    @Html.TextBoxFor(x => x.Description) <br />
    @Html.LabelFor(x => x.WhoWith) <br />
    @Html.TextBoxFor(x => x.WhoWith) <br />
    @Html.LabelFor(x => x.TimeStart) <br />
    @Html.TextBoxFor(x => x.TimeStart) <br />
    @Html.LabelFor(x => x.TimeEnd) <br />
    @Html.TextBoxFor(x => x.TimeEnd) <br />
    <button type="submit" class="btn btn-default">Add Appointment</button>
</form>

